  787|  4.74M|    secp256k1_fe_mul(&s2, &b->y, &zz);                  /* s2 = Y2*Z1^2 (1) */
  ------------------
  |  |   93|  4.74M|#  define secp256k1_fe_mul secp256k1_fe_impl_mul
  ------------------
  788|  4.74M|    secp256k1_fe_mul(&s2, &s2, &a->z);                  /* s2 = S2 = Y2*Z1^3 (1) */
  ------------------
